{
  "name": "postman-collection-parser",
  "version": "1.0.0",
  "description": "Library that converts a Postman collection into a directory structure with test files.",
  "main": "dist/lib/parse-collection.js",
  "types": "dist/lib/parse-collection.d.js",
  "scripts": {
    "compile": "../node_modules/.bin/tsc --build tsconfig.production.json",
    "test": "../node_modules/.bin/jest",
    "example-gen": "../node_modules/.bin/ts-node -r tsconfig-paths/register ts/src/examples/ts/conduit-example-typed.ts",
    "example-test": "../node_modules/.bin/jest ts/src/examples/ts/out/tests/Conduit/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pvillela/postman-collection-parser.git"
  },
  "keywords": [
    "api",
    "testing"
  ],
  "author": "Paulo Villela",
  "license": "MIT",
  "dependencies": {
    "newman": "^5.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^28.1.4",
    "@types/newman": "^5.3.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.23.5",
    "typedoc-plugin-markdown": "^3.13.2",
    "typescript": "^4.7.4"
  }
}
